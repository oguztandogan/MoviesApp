pod 'RxSwift'
pod 'SwinjectAutoregistration'
pod 'Firebase/Analytics'
pod 'Firebase/Core'
pod 'Firebase/RemoteConfig'
pod 'Firebase/Crashlytics'
pod 'lottie-ios'
end

target 'MovieAppWebService' do
   workspace 'MovieApp'
   project 'MovieAppWebService/MovieAppWebService.xcodeproj'
   use_frameworks!
pod 'Alamofire'
pod 'RxSwift'

end